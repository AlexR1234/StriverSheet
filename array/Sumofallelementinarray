#include <iostream>
using namespace std;
int sumofarray(int arr[],int n){
  int store=0;
  for(int i=0;i<n;i++){
    store=store+arr[i];
    
  }
  return store;
}
int main() {
 int n;
  cout<<"Enter the size of array"<<endl;
  cin>>n;
  int arr[n];
  cout<<"Enter the elements of array"<<endl;
  for(int i=0;i<n;i++){
    cin>>arr[i];
  }
  cout<<sumofarray(arr,n);
}
